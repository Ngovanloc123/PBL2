{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.o": true,
    "**/*.exe": true
  },
  "C_Cpp.errorSquiggles": "disabled",
  "files.associations": {
    "list": "cpp",
    "*.tcc": "cpp",
    "array": "cpp",
    "string": "cpp",
    "string_view": "cpp"
  }
}
